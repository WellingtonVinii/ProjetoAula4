{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "index.js"
    },
    { 
      "src": "/login.html", 
      "dest": "public/login.html" 
    }
  ]
}
