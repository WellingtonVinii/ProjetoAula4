{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "rewrites": [
      { "source": "/login.html", "destination": "/public/login.html" }
  ]

}
